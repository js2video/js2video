{
  "name": "js2video",
  "version": "0.0.107",
  "license": "MIT",
  "author": {
    "name": "@pkl",
    "url": "https://github.com/pkl"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./template-ref": {
      "types": "./dist/types/template-ref.d.ts"
    },
    "./server": {
      "types": "./dist/types/lib/export-video.d.ts",
      "import": "./src/lib/export-video.js"
    }
  },
  "files": [
    "src/lib",
    "dist/types",
    "dist/index.js",
    "dist/index.css",
    "public/puppeteer.html",
    "public/puppeteer.js",
    "public/webfont.js"
  ],
  "engines": {
    "node": ">=20.17.0"
  },
  "overrides": {
    "form-data": "4.0.4",
    "tar-fs": "3.0.9",
    "cross-spawn": "7.0.5",
    "esbuild": "0.25.9"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite --config vite.config.app.js --force",
    "dev:server": "node --watch ./server.js",
    "dev:server-client": "npm run build-server-client -- --watch",
    "dev:docs": "vitepress dev docs",
    "build": "run-s build:*",
    "build:clean": "rimraf types",
    "build:lib": "vite build --config vite.config.lib.js",
    "build:types": "tsc --project tsconfig.types.json",
    "build:rollup": "rollup -c rollup.config.js",
    "build:css": "tailwindcss -i ./index.css -o ./dist/index.css",
    "build:app": "vite build --config vite.config.app.js",
    "build:typedoc": "typedoc",
    "build:docs": "vitepress build docs",
    "build-server-client": "esbuild ./src/lib/video-template --format=esm --bundle --minify --outfile=./video-template.js",
    "release": "node ./scripts/release.js",
    "preview": "run-s preview:*",
    "preview:app": "vite preview --config vite.config.app.js",
    "preview:server": "node --watch ./server.js",
    "postinstall": "npm run build-server-client"
  },
  "devDependencies": {
    "@auth0/auth0-react": "2.3.0",
    "@codemirror/lang-javascript": "6.2.2",
    "@headlessui/react": "2.2.4",
    "@lezer/highlight": "1.2.1",
    "@types/react": "18.3.11",
    "@uiw/codemirror-theme-dracula": "4.23.6",
    "@uiw/react-codemirror": "4.23.6",
    "@vitejs/plugin-react": "5.0.2",
    "autoprefixer": "10.4.20",
    "handlebars": "4.7.8",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.49",
    "react-resizable-panels": "2.1.7",
    "rimraf": "6.0.1",
    "rollup": "4.24.4",
    "rollup-plugin-dts": "6.1.1",
    "tailwindcss": "3.4.15",
    "typedoc": "0.26.11",
    "typescript": "5.6.3",
    "vite": "6.3.6",
    "vite-plugin-externalize-deps": "0.9.0",
    "vitepress": "1.6.4"
  },
  "peerDependencies": {
    "puppeteer": "23.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "peerDependenciesMeta": {
    "puppeteer": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@lottiefiles/dotlottie-web": "0.38.1",
    "@radix-ui/react-slider": "1.2.1",
    "@xzdarcy/react-timeline-editor": "0.1.9",
    "canvas-confetti": "1.9.3",
    "clsx": "2.1.1",
    "crunker": "2.4.0",
    "d3": "7.9.0",
    "esbuild": "0.25.9",
    "fabric": "6.4.3",
    "gsap": "3.12.5",
    "lucide-react": "0.459.0",
    "mediabunny": "1.14.1",
    "nanoid": "5.0.9",
    "pixi-filters": "6.0.5",
    "pixi.js": "8.4.1",
    "tailwind-merge": "2.5.4",
    "zod": "3.23.8"
  }
}
